@model IEnumerable<Cliente>


<h1> Cliente Index </h1>
<a asp-action="Create">Agregar Cliente</a>
<table class="table">
    <thead>
         <tr>
            <th>
                <label>ID</label>
            </th>
            <th>
                <label>DNI</label>
            </th>
            <th>
                <label>Nombre</label>
            </th>
            <th>
                <label>Apellido</label>
            </th>
            <th>
                <label>Direccion</label>   
            </th>
            <th>
                <label>Telefono</label>
            </th>
            <th>
                <label>Email</label>
            </th>
            <th>

            </th>
        </tr>
    </thead>
    @foreach (var cliente in Model){
        <tr>
            <td>
                <label>@cliente.Id</label>
            </td>
            <td>
                <label>@cliente.DNI</label>
            </td>
            <td>
                <label>@cliente.Nombre</label>
            </td>
            <td>
                <label>@cliente.Apellido</label>
            </td>
            <td>
                <label>@cliente.Direccion</label>
            </td>
            <td>
                <label>@cliente.Telefono</label>
            </td>
            <td>
                <label>@cliente.Email</label>
            </td>
        </tr>
        <tr>
            <td>
                <a asp-action="Edit" asp-route-id="@cliente.Id">Editar</a>
            </td>
            <td>
                <a asp-action="Delete" asp-route-id="@cliente.Id">Eliminar</a>
            </td>
        </tr>
    }
</table>